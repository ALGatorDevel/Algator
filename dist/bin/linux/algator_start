#!/bin/bash

tID=`docker container ls | grep algatorweb | cut -f 1 -d" "`

if [ ${#tID} -gt 2 ]
then 
  echo "Algator docker container is already running."

  echo
  echo "Press ENTER key to continue"
  read
else	
    docker run -v $ALGATOR_ROOT/data_root:/home/algator/ALGATOR_ROOT/data_root -p 8081:8081 --env TPY=L algator/algatorweb  &
    read
fi

